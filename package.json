{
  "name": "document-viewer",
  "version": "0.0.1",
  "description": "Document viewer for React and vanilla JavaScript",
  "main": "dist/lib/index.js",
  "module": "dist/es2015/index.js",
  "types": "dist/es2015/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run build-cjs && npm run copy-worker",
    "build-cjs": "tsc -p tsconfig.commonjs.json",
    "copy-worker": "cp node_modules/pdfjs-dist/build/pdf.worker.min.js dist/worker/",
    "build-example": "npm run build && tsc -p tsconfig.example.json && npm run webpack",
    "webpack": "webpack",
    "clean": "rimraf ./dist",
    "lint:no-fix": "eslint --config='.eslintrc.js' './src/*.{ts,tsx}' './test/*.{ts,tsx}'",
    "lint": "npm run lint:no-fix -- --fix",
    "prepublishOnly": "npm run clean && npm run build && npm run test -- --ci --silent && npm run lint:no-fix -- --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mblink/document-viewer.git"
  },
  "keywords": [
    "document",
    "viewer",
    "pdf",
    "doc",
    "docx"
  ],
  "contributors": [
    {
      "name": "Justin Leider",
      "email": "jleider@users.noreply.github.com"
    },
    {
      "name": "Kyle Sutton"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mblink/document-viewer/issues"
  },
  "homepage": "https://github.com/mblink/document-viewer#readme",
  "devDependencies": {
    "@testing-library/react-hooks": "~7.0.2",
    "@types/jest": "~27.4.1",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "canvas": "^2.9.3",
    "core-js": "^3.23.3",
    "eslint": "~8.10.0",
    "jest": "~28.1.1",
    "jest-environment-jsdom": "~28.1.1",
    "process": "^0.11.10",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "~16.14.0",
    "smoothscroll-polyfill": "^0.4.4",
    "ts-jest": "~28.0.5",
    "ts-node": "10.4.0",
    "typescript": "4.6.2",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "pdfjs-dist": "^2.14.305"
  }
}
